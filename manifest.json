{
  "manifest_version": 3,
  "name": "BruinLearn Helper",
  "version": "1.4",
  "description": "Scrapes and saves class rosters from BruinLearn, with Markdown notes.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://bruinlearn.ucla.edu/courses/*/users"],
      "js": ["marked.min.js", "content.js"],
      "run_at": "document_idle"
    }
  ],
  "options_page": "settings.html",
  "action": {
    "default_title": "Open Saved Rosters"
  }}